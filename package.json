{
  "name": "n8n-nodes-reftab",
  "version": "1.0.5",
  "description": "n8n node for Reftab Asset Management API",
  "keywords": [
    "n8n-community-node-package",
    "n8n",
    "reftab",
    "asset-management",
    "inventory"
  ],
  "license": "MIT",
  "homepage": "https://github.com/reftab/n8n-nodes-reftab",
  "author": {
    "name": "Michael Plocher",
    "email": "michael.plocher@reftab.com"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/reftab/n8n-nodes-reftab.git"
  },
  "main": "index.js",
  "scripts": {
    "build": "tsc && gulp build:icons",
    "dev": "tsc --watch",
    "format": "prettier nodes credentials --write",
    "lint": "eslint nodes credentials",
    "lintfix": "eslint nodes credentials --fix",
    "prepublishOnly": "npm run build"
  },
  "files": [
    "dist"
  ],
  "n8n": {
    "n8nNodesApiVersion": 1,
    "credentials": [
      "dist/credentials/ReftabApi.credentials.js"
    ],
    "nodes": [
      "dist/nodes/Reftab/Reftab.node.js"
    ]
  },
  "devDependencies": {
    "@eslint/js": "^9.39.1",
    "@types/node": "^24.10.1",
    "eslint": "^9.39.1",
    "gulp": "^5.0.1",
    "n8n-workflow": "*",
    "prettier": "^3.7.3",
    "typescript": "^5.9.3",
    "typescript-eslint": "^8.48.0"
  },
  "peerDependencies": {
    "n8n-workflow": "*"
  }
}
